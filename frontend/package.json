{
  "name": "wiring-record-frontend",
  "version": "1.0.0",
  "description": "综合布线记录管理系统 - 前端应用",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "build.bat",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write \"src/**/*.{vue,js,ts,jsx,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{vue,js,ts,jsx,tsx,json,css,scss,md}\"",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "axios": "^1.6.5",
    "element-plus": "^2.5.4",
    "@element-plus/icons-vue": "^2.3.1",
    "pinia": "^2.1.7"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vue-tsc": "^2.0.0",
    "sass": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.1.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "vitest": "^1.1.3",
    "@vue/test-utils": "^2.4.3",
    "jsdom": "^23.2.0",
    "@vitest/coverage-v8": "^1.1.3",
    "@vitest/ui": "^1.1.3"
  },
  "keywords": [
    "wiring",
    "record",
    "management",
    "vue",
    "vue3",
    "typescript",
    "element-plus",
    "vite",
    "frontend"
  ],
  "author": "综合布线记录管理系统团队",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/sjzx-zonghebuxian.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/sjzx-zonghebuxian/issues"
  },
  "homepage": "https://github.com/your-username/sjzx-zonghebuxian#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
